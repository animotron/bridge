def geo-app

    (^app)

    (title "Geo!")

    (css any leaflet.css)

    (style
        ".leaflet-tile {max-width: none; max-height: none;}"
    )

    (js
        (any leaflet.min.js)
        (any geo.js)
    )

    (script "$(function(){$.geo(51.31289, 37.855317, 13);});")

    (frame
        (\div (@id "map") (@class "frame"))
    )

